{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ugodffactory"
		},
		"AzureSqlDatabaseLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLinkedService'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"SqlServerLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerLinkedService'"
		},
		"SqlServerLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServerLinkedService'"
		},
		"SqlServerLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ugoon"
		},
		"AzureBlobStorageManagedprivateendPoint_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/59eeaea9-322c-4fea-ad62-2faf27b35902/resourceGroups/dwhRg/providers/Microsoft.Storage/storageAccounts/ugostorage"
		},
		"AzureBlobStorageManagedprivateendPoint_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"AzureBlobStorageManagedprivateendPoint_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"ugostorage.blob.core.windows.net"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ugodffactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabaseLinkedService_connectionString": {
						"value": "[parameters('AzureSqlDatabaseLinkedService_connectionString')]"
					},
					"SqlServerLinkedService_connectionString": {
						"value": "[parameters('SqlServerLinkedService_connectionString')]"
					},
					"SqlServerLinkedService_password": {
						"value": "[parameters('SqlServerLinkedService_password')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"SqlServerLinkedService_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerLinkedService_properties_typeProperties_userName')]"
					},
					"AzureBlobStorageManagedprivateendPoint_properties_privateLinkResourceId": {
						"value": "[parameters('AzureBlobStorageManagedprivateendPoint_properties_privateLinkResourceId')]"
					},
					"AzureBlobStorageManagedprivateendPoint_properties_groupId": {
						"value": "[parameters('AzureBlobStorageManagedprivateendPoint_properties_groupId')]"
					},
					"AzureBlobStorageManagedprivateendPoint_properties_fqdns": {
						"value": "[parameters('AzureBlobStorageManagedprivateendPoint_properties_fqdns')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ugodffactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ugodffactory_ArmTemplate_0')]"
			]
		}
	]
}